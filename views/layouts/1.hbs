        <script>
            const closer = document.querySelector('.notification_close');
            const billy_notifs = document.querySelectorAll('.notif_needs_closing');
            const notifs_list = document.querySelector('#notifications_list');
            const notifs_none = document.querySelector('#notifications_none');

            if (notifs_list.children.length <= 1) {
                notifs_none.classList.remove('sr-only');
            } else {
                notifs_none.classList.add('sr-only');
            }

            closer.addEventListener('click', async () => {
                billy_notifs.forEach(async (item) => {
                    item.remove();
                });

                notifs_none.classList.remove('sr-only');

                $.ajax({
                    type: 'GET',
                    url: 'https://rustlands.net/api/notifications',
                    dataType: 'json',
                    success: async (res) => {
                        console.log('Done!')
                    },
                    error: async (res) => {
                        return console.error('Unable to remove notification, possible server issue.');
                    }
                });
            });
        </script>

